  <body>
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 38.0293, lng: -78.4767},
          zoom: 13
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAM9lrQu7c3OEOC3NwtPipffPfsck6eQSw&callback=initMap"
    async defer></script>
  </body>
